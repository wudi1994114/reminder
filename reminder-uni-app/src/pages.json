{
	"easycom": {
		"autoscan": true,
		"custom": {
			"^login-form": "@/components/LoginForm.vue",
			"^confirm-dialog": "@/components/ConfirmDialog.vue",
			"^confirm-modal": "@/components/ConfirmModal.vue",
			"^picker-modal": "@/components/PickerModal.vue",
			"^cron-expression-picker": "@/components/CronExpressionPicker.vue",
			"^datetime-picker": "@/components/datetime-picker/datetime-picker.vue",
			"^trigger-preview": "@/components/trigger-preview/trigger-preview.vue",
			"^unified-time-picker": "@/components/unified-time-picker/unified-time-picker.vue",
			"^v-calendar": "@/components/v-calendar/v-calendar.vue"
		}
	},
	"pages": [
		{
			"path": "pages/index/index",
			"style": {
				"navigationBarTitleText": "提醒首页"
			}
		},
		{
			"path": "pages/create/create",
			"style": {
				"navigationBarTitleText": "创建提醒"
			}
		},
		{
			"path": "pages/create-complex/create-complex",
			"style": {
				"navigationBarTitleText": "创建复杂提醒"
			}
		},
		{
			"path": "pages/detail/detail",
			"style": {
				"navigationBarTitleText": "提醒详情"
			}
		},
		{
			"path": "pages/calendar/calendar",
			"style": {
				"navigationBarTitleText": "日程"
			}
		},
		{
			"path": "pages/mine/mine",
			"style": {
				"navigationBarTitleText": "我的"
			}
		},
		{
			"path": "pages/login/login",
			"style": {
				"navigationBarTitleText": "登录"
			}
		},
		{
			"path": "pages/register/register",
			"style": {
				"navigationBarTitleText": "注册"
			}
		},
		{
			"path": "pages/profile/edit",
			"style": {
				"navigationBarTitleText": "编辑资料"
			}
		},
		{
			"path": "pages/settings/about",
			"style": {
				"navigationBarTitleText": "关于应用"
			}
		},
		{
			"path": "pages/settings/notification",
			"style": {
				"navigationBarTitleText": "提醒设置",
				"navigationStyle": "custom"
			}
		},
		{
			"path": "pages/settings/test-preferences",
			"style": {
				"navigationBarTitleText": "API测试"
			}
		}
	],
	"globalStyle": {
		"navigationBarTextStyle": "black",
		"navigationBarTitleText": "提醒应用",
		"navigationBarBackgroundColor": "#F8F8F8",
		"backgroundColor": "#F8F8F8"
	},
	"tabBar": {
		"color": "#7A7E83",
		"selectedColor": "#3cc51f",
		"borderStyle": "black",
		"backgroundColor": "#ffffff",
		"list": [
			{
				"pagePath": "pages/index/index",
				"iconPath": "static/images/home.png",
				"selectedIconPath": "static/images/home-active.png",
				"text": "提醒"
			},
			{
				"pagePath": "pages/calendar/calendar",
				"iconPath": "static/images/calendar.png",
				"selectedIconPath": "static/images/calendar-active.png",
				"text": "日历"
			},
			{
				"pagePath": "pages/mine/mine",
				"iconPath": "static/images/mine.png",
				"selectedIconPath": "static/images/mine-active.png",
				"text": "我的"
			}
		]
	}
}
